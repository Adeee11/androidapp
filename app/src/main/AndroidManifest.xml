<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.androidbanking">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AndroidBanking">
        <activity android:name=".AccountSummary"></activity>
        <activity android:name=".UpdatePersonalDetails" />
        <activity android:name=".DeleteAccount" />
        <activity android:name=".CreateAccount" />
        <activity android:name=".DepostMoney" />
        <activity android:name=".AllAccountOptions" />
        <activity android:name=".PayUtils" />
        <activity android:name=".TransferMoney" />
        <activity android:name=".Signuppage" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>